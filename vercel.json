{
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node"
    }
  },
  "includeFiles": [
    "app.js",
    "db.js",  
    "config/index.js",
    "routes/index.routes.js",
    "models/Client.model.js"  
  ]
}
